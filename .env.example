# JWT Secret (change in production)
JWT_SECRET=your-secure-jwt-secret-here
# Session TTL in seconds (7 days = 604800)
SESSION_TTL=604800
# Cloudflare D1 Database
# Run: wrangler d1 create job-portal-db
# Then update wrangler.jsonc with the database_id
D1_DATABASE_ID=your-database-id
# Cloudflare KV Namespaces
# Run: wrangler kv namespace create SESSIONS
# Run: wrangler kv namespace create CACHE
# Then update wrangler.jsonc with the namespace ids
KV_SESSIONS_ID=your-sessions-kv-id
KV_CACHE_ID=your-cache-kv-id